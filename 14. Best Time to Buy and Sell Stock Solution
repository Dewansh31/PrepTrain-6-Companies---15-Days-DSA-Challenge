#include <bits/stdc++.h>

int maximumProfit(vector<int> &prices) {

  int mini = prices[0];

  int profit = 0;

  int maxi = INT_MIN;

  for (int i = 1; i < prices.size(); i++) {

    int cost = prices[i] - mini;

    profit = max(profit, cost);

    mini = min(mini, prices[i]);
  }

  return profit;
}
