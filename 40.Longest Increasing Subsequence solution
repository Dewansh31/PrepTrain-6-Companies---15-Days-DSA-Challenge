#include<bits/stdc++.h>

int longestIncreasingSubsequence(int arr[], int n)

{

    // Write Your Code here

    vector<int> ans;

    for(int i=0;i<n;i++){

        if(ans.size()==0||ans[ans.size()-1]<arr[i]){

            ans.push_back(arr[i]);

        }

        else{

            auto it=lower_bound(ans.begin(),ans.end(),arr[i]);

            *it=arr[i];

        }

    }

    return ans.size();

}
