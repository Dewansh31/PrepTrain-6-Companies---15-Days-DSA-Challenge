#include <bits/stdc++.h>
vector<int> maximumsumSubarray(vector<int> &arr, int n) {
  int sum = INT_MIN;
  int maxi = INT_MIN;
  int start = 0, ansStart = 0, ansEnd = 0;
  for (int i = 0; i < n; i++) {
    if (sum < 0) {
      sum = arr[i];
      start = i;
    } else
      sum += arr[i];
    if (sum > maxi) {
      maxi = sum;
      ansStart = start;
      ansEnd = i;
    } else if (sum == maxi && ansEnd - ansStart < i - start) {
      ansStart = start;
      ansEnd = i;
    }
  }
  vector<int> ans;
  for (int i = ansStart; i <= ansEnd; i++)
    ans.push_back(arr[i]);
  return ans;
}
